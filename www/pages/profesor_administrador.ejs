             <input type="text" name="amaterno" class="form-control">
            </div>
            <div class="form-group">
                    <label>Sexo</label>
                    <select class="custom-select" name="sexo">
                            <option value="hombre">Hombre</option>
                            <option value="mujer">Mujer</option>

                         </select>
                    </div>
                    <div class="form-group">
