                                                      </div>
              <button type="button" onclick="toregister()" class="btn btn-primary btn-block">Guardar</button>
      </form>
    </div>

    <script>

        toregister= () => {
                $.ajax({
                url:'/profesor_administrador',
                method:'post',
                data:$('#form_profesor_administrador').serialize(),
                success:(data)=>{
                        alert(data.message);
                },
                error:(err)=>{
        
                }
        });
        }
        
        </script>
        
